<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="root" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.cmjd.project.view.controller.PaymentFormController">
   <children>
      <AnchorPane fx:id="root2" layoutX="10.0" layoutY="10.0" prefHeight="504.0" prefWidth="770.0">
         <children>
            <Label fx:id="lblPayments" alignment="CENTER" layoutY="-1.0" prefHeight="80.0" prefWidth="601.0" style="-fx-background-color: orange;" text="Payments" textAlignment="CENTER" textFill="#fff8f8">
               <font>
                  <Font name="System Bold" size="36.0" />
               </font>
            </Label>
            <Button fx:id="btnBack" layoutX="639.0" layoutY="27.0" mnemonicParsing="false" onAction="#btnBackOnAction" prefHeight="39.0" prefWidth="121.0" style="-fx-background-color: red;" text="Back">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Button>
            <Label fx:id="lblSelect1" alignment="CENTER" layoutX="185.0" layoutY="351.0" prefHeight="59.0" prefWidth="402.0" text="Select" textAlignment="CENTER" textFill="#d312e4">
               <font>
                  <Font name="System Bold" size="22.0" />
               </font>
            </Label>
            <Label fx:id="lblDescription1" alignment="CENTER" layoutX="214.0" layoutY="410.0" prefHeight="45.0" prefWidth="374.0" text="Please select one of the above operations to proceed" />
            <ImageView fx:id="imageMembershipFeePay" fitHeight="200.0" fitWidth="200.0" layoutX="539.0" layoutY="132.0" onMouseClicked="#navigate" onMouseEntered="#playMouseEnterAnimation" onMouseExited="#playMouseExitAnimation" preserveRatio="true">
               <image>
                  <Image url="@../images/membership-fee.png" />
               </image>
            </ImageView>
            <ImageView fx:id="imageLostPay" fitHeight="200.0" fitWidth="159.0" layoutX="306.0" layoutY="132.0" onMouseClicked="#navigate" onMouseEntered="#playMouseEnterAnimation" onMouseExited="#playMouseExitAnimation" preserveRatio="true">
               <image>
                  <Image url="@../images/LostPay.png" />
               </image>
            </ImageView>
            <ImageView fx:id="imageFinePay" fitHeight="200.0" fitWidth="200.0" layoutX="9.0" layoutY="132.0" onMouseClicked="#navigate" onMouseEntered="#playMouseEnterAnimation" onMouseExited="#playMouseExitAnimation" preserveRatio="true">
               <image>
                  <Image url="@../images/FinePay.png" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
